{
  "name": "flash-element-ui",
  "version": "0.3.7",
  "description": "基于elementUi的补充",
  "author": "jixuanyu <596041986@qq.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "element-ui",
    "selectTree"
  ],
  "scripts": {
